<CreatePurchaseOrderRequest>
  <RequestProperties>
    <SchemaVersion>?</SchemaVersion>
    <Locale>?</Locale>
    <DateAndTimes subClass="?">?</DateAndTimes>
  </RequestProperties>
  <CustomerNo>?</CustomerNo>
  <!--Optional:-->
